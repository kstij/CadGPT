# Track large files with Git LFS
venv/Lib/site-packages/cv2/cv2.pyd filter=lfs diff=lfs merge=lfs -text
venv/Lib/site-packages/open3d/cpu/pybind.cp311-win_amd64.pyd filter=lfs diff=lfs merge=lfs -text
venv/Lib/site-packages/torch/lib/torch_cpu.dll filter=lfs diff=lfs merge=lfs -text
venv/Lib/site-packages/torch/lib/dnnl.lib filter=lfs diff=lfs merge=lfs -text

# Track all .pyd files
*.pyd filter=lfs diff=lfs merge=lfs -text

# Track all .dll files
*.dll filter=lfs diff=lfs merge=lfs -text

# Track all .lib files
*.lib filter=lfs diff=lfs merge=lfs -text 